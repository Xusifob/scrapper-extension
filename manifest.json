{
  "manifest_version": 2,

  "name": "Xusifob Web Scrapper",
  "description": "This extension Goes with Bastien Malahieude Web Scrapper and allows you to get data from Google easily",
  "version": "1.0",

  "background" : {
    "scripts" : ["libs/jquery-3.3.1.min.js",
      "background/background.js",
      "background/website.js",
      "background/google.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },

  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },

  "permissions": [
    "activeTab",
    "storage",
    "background",
    "tabs",
    "*://*.facebook.com/*",
    "*://www.google.com/*",
    "*://www.google.fr/*",
    "*://www.google.com.sg/*"

  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.malahieude.net/*",
        "*://localhost/*",
        "*://*.google.com.sg/*",
        "*://www.google.fr/*",
        "*://www.google.com/*",
        "*://www.google.es/*"
      ],
      "js": [
        "content/content.js",
        "content/google.js"
      ]
    }
  ]
}
